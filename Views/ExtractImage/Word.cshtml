
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>保存时获取word文档中的图片</title>
</head>
<body>
    <script type="text/javascript">
        function Save() {
            document.getElementById("PageOfficeCtrl1").WebSave();
            var value = document.getElementById("PageOfficeCtrl1").CustomSaveResult;
            document.getElementById("PageOfficeCtrl1").Alert(decodeURIComponent(value));
        }
    </script>
    <div id="div1" style="width: auto; height: 700px;">
        @Html.Raw(ViewBag.POCtrl)
    </div>
</body>
</html>